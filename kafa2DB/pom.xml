    <groupId>cn.javaguide</groupId>
    <artifactId>springboot-kafka-01-send-objects</artifactId>
    <version>0.0.1-SNAPSHOT</version>
    <name>springboot-kafka-01-send-objects</name>
    <description>Demo project for Spring Boot</description>

    <properties>
        <java.version>1.8</java.version>
